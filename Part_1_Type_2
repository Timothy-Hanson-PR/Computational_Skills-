# non parametric bootstrap for Poisson lambda: Type 2 
set.seed(42) 
B <- 5000 
lambda_star <- numeric(B) 
for (b in 1:B) { 
daily_counts_star <- sample(daily_counts_type2, n_days, replace = TRUE) 
